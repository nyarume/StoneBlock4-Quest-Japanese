	quest.2EA7D8A9632F845F.title: "&f自動構築: &3エンダリウムアップグレード"
	quest.2EC3F054AB506916.title: "上級マシンブロック取引"
	quest.2ECAD5F752E445F1.quest_desc: [
		"[\"\", { \"text\": \"プロメチウム\", \"color\": \"#FCD205\"}, \" は、\", { \"text\": \"アトミックフォージ\", \"color\":\"#FF55FF\"}, \" でのみ生成できる上級素材です。\" ]"
		""
		"[\"この強力な資源は、\", {\"text\": \"プロメチウムツール\", \"color\":\"#FCD205\"}, \" や \", {\"text\": \"アルティメットマシンコア\", \"color\":\"#FF55FF\"}, \" の作成に不可欠です。\"]"
